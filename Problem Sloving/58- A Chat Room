#include<bits/stdc++.h>
using namespace std;

int main()
{
 string input , w = "hellow";
    

cin >> input;
int j =0;


for(int i =0; i< input.size(); i++)
{

    if(w[j] == input[i])
    {
        j++;
        if( j==5)
      
        {
          
              cout<< "YES" <<endl;
              return 0;
    
        }

    }      



}

  cout << "NO" <<endl;

  return 0;

}
