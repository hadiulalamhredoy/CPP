#include<bits/stdc++.h>
using namespace std;
int main()
{
     int n , p, vx;
     cin>> n;
     set<int>levels;

     for(int i = 0; i<p; i++)
     {
        cin >>vx;
        levels.insert(vx);
        
     }
      
      int q , vy;
      cin >> vy;

      
     for(int i = 0; i<q; i++)
     {
        cin >>vy;
        levels.insert(vy);
    }
    

     if(levels.size() == n)
     {
        cout << " i become the guy" <<endl;
     }


     else 
     {
         cout <<"oh , my keyboard" <<endl; 
       }




}



