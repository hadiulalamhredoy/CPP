#include<bits/stdc++.h>
using namespace std;
int main()
{
   long long a;
   int lucky_count =0;
   cin >> a;
   while(a! =0)
   {
       if(a % 10 ==4 || a%10 == 7)
       {
           lucky_count++;
           
       }
       
           a =a/10;
   }
    
    if(lucky_count == 7 || lucky_count == 4)
    {
        cout << "YES" << endl;
    }

    else 
    {
        cout << "NO" << endl;
    }
    
      return 0;
    
}
