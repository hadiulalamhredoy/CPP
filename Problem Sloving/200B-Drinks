#include<bits/stdc++.h>
using namespace std;
int main()
{
    double n , v , sum=0,result;
    cin>> n;
    for(int i  = 0 ; i<n ; i++)
    {
        
        cin>> v;
        sum+=v;
        result = (sum*100)/(n*100);
        cout <<fixed << setprecision(12) <<result <<endl;
    }
       return 0;
    
}
