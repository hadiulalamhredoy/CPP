#include<bits/stdc++.h>
using namespace  std;
int main ()
{ 
       
     long long a,b,c,min=0, max = 0;
     cin >>  a >> b >> c ;

        if(a<=b && a<=c){
            min =a;
        }else if(b<=a && b<=c){
            min = b;
        }else{
             min = c;
        }
 
        if(a>=b  && a>=c){
            max = a;
      }else if (b>=a && b>=c){
           max = b;
      }else {
            max = c;
      }
   
        cout << min <<" "  <<max <<endl;
       return 0; 
}


