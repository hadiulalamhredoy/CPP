#include<bits/stdc++.h>
using namespace  std;
int main ()
{ 
     int n;
     cin>>n;
     int arr[n];

     for(int i = 0; i < n; i++){
        int p;
        cin>>p;
        arr[i] = p;

     }
      
          int q;
          cin>>q;

          for(int i = 0; i < q; i++) {
          int a , b ;
          cin >> a >> b;

          
          reverse(arr+a, arr+b);

          }

          for(auto i: arr) cout << i << " ";
          cout << '\n';
       
       return 0; 
}





