#include<bits/stdc++.h>
using namespace std;
int main(){
    int size;
    cin>>size;
    int arr[size];
    for(int i = 0; i<size;i++){
        cin>>arr[i];
    }
    int test ;
    cin>>test;

    for(int i=0;i<test;i++){
        int start,end,num;
        int count = 0;
        cin>>start>>end>>num;
        for(int j=start;j<end;j++){
            if(arr[j]==num){
                count++;
            }
        }
        cout<<count<<"\n";
    }

    return 0;
}
