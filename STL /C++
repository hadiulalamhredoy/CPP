
#include <bits/stdc++.h>
using namespace std;
const int N = 2e5+7;
int main() {
   
   int q;
    cin >> q;
    vector<int>a;
  
   

   for(int j = 1; j<=q; j++){
        int type;
        cin >> type;
       
        if(type == 0){
            int x;
            cin >> x;
            a.push_back(x);
 
            
        } else if(type == 1){
             int p;
             cin >> p;
          
             cout << a[p] <<endl;
       }else {
             a.pop_back();
         }

   }

}
