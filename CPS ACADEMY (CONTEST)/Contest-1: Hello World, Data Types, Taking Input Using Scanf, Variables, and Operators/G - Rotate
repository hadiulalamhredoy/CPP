#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n); 

    int a = n / 100;        
    int b = (n / 10) % 10;  
    int c = n % 10;       

    int num1 = n;           
    int num2 = b * 100 + c * 10 + a;
    int num3 = c * 100 + a * 10 + b;

    int sum = num1 + num2 + num3;

    printf("%d\n", sum);

    return 0;
}
